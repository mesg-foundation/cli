{
  "name": "mesg-cli",
  "description": "MESG CLI",
  "version": "1.0.4",
  "author": "Anthony ESTEBE @antho1404",
  "bin": {
    "mesg-cli": "./bin/run"
  },
  "bugs": "https://github.com/mesg-foundation/cli/issues",
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "@oclif/command": "^1.5.13",
    "@oclif/config": "^1.13.0",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.1.0",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-update": "^1.3.9",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@types/js-yaml": "^3.12.1",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.2.1",
    "download-tarball": "^2.0.0",
    "git-clone": "^0.1.0",
    "grpc": "^1.21.1",
    "handlebars": "^4.1.2",
    "hosted-git-info": "^2.7.1",
    "ignore": "^5.1.2",
    "inquirer": "^6.3.1",
    "ipfs-http-client": "^29.1.1",
    "is-git-url": "^1.0.0",
    "js-yaml": "^3.13.1",
    "mesg-js": "^3.0.0",
    "node-docker-api": "^1.1.22",
    "rimraf": "^2.6.3",
    "tar": "^4.4.8",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/hosted-git-info": "^2.7.0",
    "@types/inquirer": "0.0.43",
    "@types/is-git-url": "^1.0.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.6",
    "@types/rimraf": "^2.0.2",
    "@types/tar": "^4.0.0",
    "@types/uuid": "^3.4.4",
    "@types/validator": "^10.11.0",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/assets",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mesg-foundation/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mesg-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topics": {
      "service": {
        "description": "Manage MESG Services"
      },
      "daemon": {
        "description": "Manage MESG Engine"
      },
      "marketplace": {
        "description": "Let you interact with the MESG Marketplace"
      },
      "account": {
        "description": "Manage your MESG Accounts"
      }
    }
  },
  "repository": "mesg-foundation/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib tsconfig.tsbuildinfo && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "oclif-dev": "oclif-dev",
    "lint": "tslint -p ./ -t stylish --fix"
  },
  "types": "lib/index.d.ts"
}
